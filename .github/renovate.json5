{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  labels: [
    "renovate"
  ],
  reviewers: [
    "rnett"
  ],
  assignees: [
    "rnett"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "/org.jetbrains.kotlin:.*/"
      ],
      "versioning": "regex",
      "allowedVersions": "/^.*$/"
    }
  ],
  enabledManagers: [
    "gradle",
    "gradle-wrapper"
  ],
}
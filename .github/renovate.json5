{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "groupSlug": "all",
  labels: [
    "renovate"
  ],
  reviewers: [
    "rnett"
  ],
  assignees: [
    "rnett"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "allowedVersions": "/.*/i"
    }
  ],
  enabledManagers: [
    "gradle",
    "gradle-wrapper"
  ],
}
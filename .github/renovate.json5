{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  labels: [
    "renovate"
  ],
  reviewers: [
    "rnett"
  ],
  assignees: [
    "rnett"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "org.jetbrains.kotlin:kotlin-gradle-plugin"
      ],
      "allowedVersions": "/.*/",
      "ignoreUnstable": false
    }
  ],
  enabledManagers: [
    "gradle",
    "gradle-wrapper"
  ],
}